generator client {
  provider = "prisma-client-js"
  output   = "../generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Score {
  sbd String @id

  toan         Float? // nullable
  ngu_van      Float?
  ngoai_ngu    Float?
  vat_li       Float?
  hoa_hoc      Float?
  sinh_hoc     Float?
  lich_su      Float?
  dia_li       Float?
  gdcd         Float?
  ma_ngoai_ngu String?
}
